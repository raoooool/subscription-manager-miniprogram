{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["ESNext", "DOM"],
    "types": ["miniprogram-api-typings"],
    "baseUrl": "./miniprogram",
    "paths": {
      "*": ["*", "typings/*"]
    },
    "strict": true,
    "noImplicitAny": false,
    "composite": true
  },
  "include": ["miniprogram/**/*.ts"],
  "exclude": ["node_modules"]
}
